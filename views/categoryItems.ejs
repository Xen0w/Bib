<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Document</title>
    </head>
    <body>
        <h1><%= category_name %> containe <%= rows %> books</h1>
        <ul>
            <% items.forEach((item)=>{ %>

            <li>
                <a href="/items/item/<%= item.item_id %>"
                    ><%= item.item_name %></a
                >
            </li>

            <% }); %>
        </ul>
        <a href="/categories/<%= category_name %>/items"
            ><button>Create new Item</button></a
        >
    </body>
</html>
